{
  "version": 2,
  "buildCommand": "cd apps/web && npm run build",
  "outputDirectory": "apps/web/.next",
  "installCommand": "npm install && cd apps/web && npm install",
  "framework": "nextjs",
  "env": {
    "NODE_VERSION": "22",
    "VERCEL": "1"
  },
  "functions": {
    "apps/web/app/api/**/*.js": {
      "runtime": "@vercel/node@3.0.0"
    }
  }
}
