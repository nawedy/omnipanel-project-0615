{
  "version": 2,
  "buildCommand": "pnpm install --frozen-lockfile && pnpm run build:web-with-deps",
  "outputDirectory": "apps/web/.next",
  "installCommand": "pnpm install --frozen-lockfile",
  "framework": "nextjs",
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "app/api/**/*.ts": {
      "runtime": "@vercel/node@5.2.2"
    }
  }
}